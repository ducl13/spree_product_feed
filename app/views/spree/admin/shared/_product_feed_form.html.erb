<div data-hook="feed_active">
 <%= f.field_container :feed_active, class: ['form-group'] do %>
    <%= f.check_box :feed_active %>
    <%= f.label :feed_active, Spree.t(:feed_active) %>
    <%= f.error_message_on :feed_active %>
 <% end %>
<%= f.field_container :feed_category, class: ['form-group'] do %>
  <%= f.label :feed_category, Spree.t(:feed_category) %>
  <%= f.text_field :feed_category, class: 'form-control' %>
  <%= f.error_message_on :feed_category %>
  <% end %>
</div>
